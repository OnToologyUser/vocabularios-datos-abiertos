        <rdfs:comment xml:lang="es">Una ciudad es un área urbana en la que predominan fundamentalmente la industria y los servicios [fuente: Wikipedia]</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.linkeddata.es/datosabiertos/def/sector-publico/territorio"/>
    </owl:Class>
    


    <!-- http://vocab.linkeddata.es/datosabiertos/def/sector-publico/territorio#CiudadAutonoma -->

    <owl:Class rdf:about="&esadm;CiudadAutonoma">
        <rdfs:label xml:lang="es">Ciudad Autónoma</rdfs:label>
        <rdfs:subClassOf rdf:resource="&esadm;Autonomia"/>
        <owl:disjointWith rdf:resource="&esadm;ComunidadAutonoma"/>
        <dcterms:issued>2014-03-28</dcterms:issued>
        <rdfs:comment xml:lang="es">Ciudad con competencias superiores a las de un municipio (pueden decretar regulaciones ejecutivas, con competencias reguladoras mayores que las de un municipio) pero inferiores a las de una comunidad autónoma (puesto que no tienen cámaras legislativas propiamente dichas) [fuente: Wikipedia]</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/derecho/vocabulario"/>
